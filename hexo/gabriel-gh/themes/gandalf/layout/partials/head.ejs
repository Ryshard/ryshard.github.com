<head>
  <link
    rel="dns-prefetch"
    href="https://gabrielgarus.s3-eu-west-1.amazonaws.com/"
  />
  <link rel="dns-prefetch" href="http://www.linkedin.com/" />

  <meta charset="utf-8" />
  <title><%- page.title %></title>
  <meta name="description" content="<%- page.description %>" />
  <meta name="identifier-url" content="<%- page.permalink %>" />
  <meta name="robots" content="index,follow" />

  <meta
    name="google-site-verification"
    content="2jMqtJKHWbtJI0laHDe4a9YwXnNG7_JYujABTtqq_tw"
  />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />

  <!-- Open Graph -->
  <meta property="og:locale" content="en" />
  <meta property="og:type" content="website" />
  <meta property="og:title" content="<%- page.title %>" />
  <meta property="og:description" content="<%- page.description %>" />
  <meta property="og:url" content="<%- page.permalink %>" />
  <meta property="og:site_name" content="<%- page.title %>" />
  <meta property="og:image" content="<%- full_url_for('/img/logo.png') %>" />
  <meta property="og:image:type" content="image/jpeg" />
  <meta property="og:image:width" content="1200" />
  <meta property="og:image:height" content="630" />
  <%- css('/css/style.css')  %> 

  <!-- Twitter -->
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:title" content="<%- page.title %>" />
  <meta name="twitter:image" content="<%- full_url_for('/img/logo.png') %>" />
  <meta name="twitter:description" content="<%- page.description %>" />
  <meta name="twitter:site" content="@Azaqi" />
  <meta name="twitter:creator" content="@Azaqi" />
</head>
